{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": "off",
    "no-inline-styles": "off",
    "disown-opener": "off",
    "button-type": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "text-wrap",
          "text-wrap: wrap",
          "backdrop-filter"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ]
  }
}